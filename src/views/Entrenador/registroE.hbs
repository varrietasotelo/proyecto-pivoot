<div class="container p-4">
    <div class="col-md-4 mx-auto">
        <div class="card-body">
            {{#if success}}
                {{>message}}
            {{/if}}
            <br>
            <h4 class="text-center">Bienvenido {{UsuNombre}}</h4>
            <br>
            <form action="/Entrenador/registroE" method="post">
                <div class="form-group">
                    <input type="text" class="form-control" name="UsuNombre" placeholder="Digite el nombre de usuario" autofocus>
                </div>
                <div class="form-group">
                    <input type="text" class="form-control" name="EntNombre" placeholder="Nombre" >
                </div>
                <div class="form-group">
                    <input type="text" class="form-control" name="EntApellido" placeholder="Apellidos" >
                </div>
                <div class="form-group">
                    <input type="number" class="form-control" name="EntDocumento" placeholder="Digite su documento de identidad" >
                </div>
                <div class="form-group">
                    <input type="text" class="form-control" name="EntDireccion" placeholder="Direccion" >
                </div>
                <div class="form-group">
                    <input type="number" class="form-control" name="EntTelefono" placeholder="Telefono" >
                </div>
                <div class="form-group">
                    <input type="email" class="form-control" name="EntCorreo" placeholder="Correo" >
                </div>
                <div class="form-group">
                    <button class="btn btn-warning btn-block">
                        Guardar
                    </button>
                {{#if right}}
                            <p class="text-success text-center">{{right}}</p>
                {{/if}}
                {{#if err2}}
                            <p class="text-danger text-center">{{err2}}</p>
                {{/if}}
                </div>
            </form>
        </div>
    </div>
</div>