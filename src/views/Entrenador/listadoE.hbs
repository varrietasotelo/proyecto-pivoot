<div class="container p-4">
    <br>
    {{#if correcto}}
        <p class="text-success text-center">{{correcto}}</p>
    {{/if}}
    <h4 class="text-center">LISTADO DE ENTRENADORES</h5>
    <br>
    <table class="table table-light table-striped table-hover">
    <thead>
        <tr>
        <th scope="col">Id</th>
        <th scope="col">Usuario</th>
        <th scope="col">Nombre</th>
        <th scope="col">Apellidos</th>
        <th scope="col">Documento</th>
        <th scope="col">Direccion</th>
        <th scope="col">Telefono</th>
        <th scope="col">Correo</th>
        </tr>
    </thead>
    <tbody>
        {{#each entrenador}}
        <tr>
        <th scope="row">{{IdEntrenador}}</th>
        <td>{{IdUsuario}}</td>
        <td>{{EntNombre}}</td>
        <td>{{EntApellido}}</td>
        <td>{{EntDocumento}}</td>
        <td>{{EntDireccion}}</td>
        <td>{{EntTelefono}}</td>
        <td>{{EntCorreo}}</td>
        </tr>
        {{/each}}
    </tbody>
</div>
